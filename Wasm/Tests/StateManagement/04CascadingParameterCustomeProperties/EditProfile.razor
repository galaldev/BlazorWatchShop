<h3>Edit user details</h3>
<form>
    <div class="form-group">
        <label>Name</label>
        <input type="text" class="form-control" @bind="Home.Name" />
    </div>
    <div class="form-group">
        <label>Email address</label>
        <input type="email" class="form-control" @bind="Home.Email" />
    </div>
</form>
    @code {
        [CascadingParameter] public Home Home { get; set; }
    }
